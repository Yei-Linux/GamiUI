{
  "name": "@gamiui/standard",
  "version": "0.1.0-alpha.108",
  "description": "GamiUI Library of standard components",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "jest",
    "test:snapshot-update": "jest --u",
    "storybook": "start-storybook -p 6006 -c .storybook watch-css -s ./src",
    "build-storybook": "build-storybook",
    "build:babel": "node ./scripts/build.js",
    "build": "tsup ./src/index.tsx --format cjs,esm --dts --minify",
    "build:esm": "tsc -p tsconfig.json --module es2015 --target es5 --outDir ./dist/esm && tscpaths -p tsconfig.json -s ./src -o ./dist/esm",
    "build:cjs": "tsc -p tsconfig.json --module commonjs --target es5 --outDir ./dist/cjs && tscpaths -p tsconfig.json -s ./src -o ./dist/cjs",
    "build:types": "tsc -p tsconfig.json -d --emitDeclarationOnly --outDir ./dist/types && tscpaths -p tsconfig.json -s ./src -o ./dist/types",
    "chromatic": "npx chromatic --project-token 7296afeee821",
    "prepare-husky": "husky install",
    "semantic-release": "semantic-release",
    "preversion": "npm run lint && npm run format",
    "prepublish": "npm run preversion && npm run build",
    "publishfinal": "npm run prepublish && npm publish",
    "publishOnly": "npm publish",
    "version": "git add . && git commit -m 'release: Release version :rocket:'",
    "postversion": "git push",
    "create-component": "node ./scripts/createComponent.js",
    "format": "prettier --write ./src",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src"
  },
  "author": "Yei Linux",
  "license": "ISC",
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "babel-plugin-replace-imports": "^1.0.2",
    "babel-plugin-emotion": "^11.0.0",
    "@emotion/babel-plugin": "^11.7.2",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.12.7",
    "@babel/preset-flow": "^7.12.1",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-controls": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-jest": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-storyshots": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@storybook/theming": "^6.4.13",
    "@storybook/addons": "^6.4.13",
    "storybook-dark-mode": "1.0.8",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.23",
    "@types/marked": "^2.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-select": "^4.0.17",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.1",
    "chromatic": "^5.8.0",
    "dotenv-webpack": "^6.0.4",
    "jest": "^26.6.3",
    "jest-svg-transformer": "^1.0.0",
    "react-docgen-typescript-plugin": "^0.6.2",
    "semantic-release": "^17.4.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1"
  },
  "dependencies": {
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@emotion/css": "^11.10.6",
    "@babel/preset-typescript": "^7.12.7",
    "autoprefixer": "^10.0.4",
    "babel-plugin-preval": "^5.0.0",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^5.2.0",
    "file-loader": "^6.2.0",
    "fs": "^0.0.1-security",
    "gsap": "^3.6.1",
    "marked": "^2.0.3",
    "module": "^1.2.5",
    "polished": "^4.1.3",
    "postcss-cli": "^8.3.0",
    "postcss-import": "^13.0.0",
    "storybook-addon-react-docgen": "^1.2.42",
    "style-loader": "^2.0.0",
    "tsconfig-paths": "^3.11.0",
    "tscpaths": "0.0.9",
    "typescript": "^4.1.2",
    "url-loader": "^4.1.1",
    "yup": "^0.32.9",
    "react-select": "^4.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Yei-Linux/GamificationStorybook.git"
  }
}